{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["airbnb", "prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "no-console": "warn",
    "func-names": "warn",
    "no-unused-vars": "warn",
    "array-callback-return": "warn",
    "no-restricted-syntax": "warn",
    "no-undef": "warn",
    "prefer-destructuring": "warn",
    "class-methods-use-this": "warn"
  }
}
